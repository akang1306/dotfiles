URxvt.scrollBar: false

! Clipboard
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.iso14755: false
URxvt.iso14755_52: false

! Font / Text
URxvt.saveLines:        10000
URxvt.xftAntialias:     true
URxvt.font:             xft:inconsolata:semibold:pixelsize=15:antialias=true:hinting=true, xft:dejavu sans mono:pixelsize=13:antialias=true,hinting=true
URxvt.boldFont:         xft:inconsolata:bold:pixelsize=15:antialias=true:hinting=true, xft:dejavu sans mono:pixelsize=13:antialias=true,hinting=true
URxvt.italicFont:       xft:inconsolata:italic:pixelsize=15:antialias=true:hinting=true, xft:dejavu sans mono:pixelsize=13:antialias=true,hinting=true
URxvt.bolditalicFont:   xft:inconsolata:semibold:italic:pixelsize=15:antialias=true:hinting=true, xft:dejavu sans mono:pixelsize=13:antialias=true,hinting=true

!Colors
!    Transparency
URxvt.depth: 32
URxvt.background: rgba:2b00/3000/3b00/c800

!     special
*.foreground:   #c0c5ce
*.background:   #2b303b
*.cursorColor:  #c0c5ce

!    black
*.color0:       #2b303b
*.color8:       #65737e

!     red
*.color1:       #bf616a
*.color9:       #bf616a

!     green
*.color2:       #a3be8c
*.color10:      #a3be8c

!     yellow
*.color3:       #ebcb8b
*.color11:      #ebcb8b

!     blue
*.color4:       #8fa1b3
*.color12:      #8fa1b3

!     magenta
*.color5:       #b48ead
*.color13:      #b48ead

!     cyan
*.color6:       #96b5b4
*.color14:      #96b5b4

!     white
*.color7:       #c0c5ce
*.color15:      #eff1f5

! Controls
URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C
